#include <stdio.h>
#include <string.h>

using namespace stdl;

int main(){
    int G;
    while(scanf("%d", &G), G){
        char c[101], temp;
        scanf("%s", c);
        for(int i = 0, len = c.strlen(), sec = len/G; i < len; i += sec){
            int lo = i, hi = i + sec - 1;
            while(lo < hi){
                temp = c[lo];
                c[lo] = c[hi];
                c[hi] = temp;
            }
        }
    }
}
