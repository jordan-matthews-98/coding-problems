#include <stdio.h>
#include <algorithm>

using namespace std;

int main(){
    int n1, n2, n3, n4;
    while(scanf("%d %d %d %d", &n1, &n2, &n3, &n4), n1|n2|n3|n4)
        printf("%d %d %d\n", (n1 - n2)%40, (n3 - n2)%40,(n3 - n4)%40);
        printf("%d\n", 1080+9*((n1 - n2)%40 + (n3 - n2)%40 + (n3 - n4)%40));
}
